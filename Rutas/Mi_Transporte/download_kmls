/")

for url in "${urls[@]}"
do
	name="$(echo "$url" | grep -Pio '(?<=/ruta/)[^/]+')"
	mid="$(curl "$url" | grep -Pi 'www\.google.com/maps' | grep -Pio '(?<=mid=)[^&]+')"

	wget -O "${name}.kml" "http://www.google.com/maps/d/kml?forcekml=1&mid=${mid}"
	if [[ $? != 0 ]];
	then
		echo "$url" >> urls_with_errors.txt
	fi
done
